<div class="inputs row-fluid" id="form_member" > 

<%= simple_form_for(@member,  :html=> {class: 'form-horizontal'}) do |f| %>
  <%= f.error_notification %>

 <div class="row-fluid">
      <div class="span12">
    <%= f.input :number, input_html: {class: "span3"} %>
    <%= f.input :name, input_html: {class: "span3"} %>
    <%= f.input :forname, input_html: {class: "span3"} %>
    <%= f.input :birthdate, input_html: {class: "span3"} %>
  </div>
 </div>

  <div class="form-actions">
    <%= f.button :submit, class: "btn-primary span2"  %>
  </div>
<% end %>
